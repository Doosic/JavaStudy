#JPA의 쿼리문이 콘솔창에 나오게 하는 방법
application.yml 에 설정을 해준다.
spring.jpa.show_sql
spring.jpa.properties.hibernate.format_sql
spring.jpa.properties.hibernate.use_sql_comments

spring:
  h2:
    console:
      enabled: true
  jpa:
    show_sql: true
    properties:
      hibernate:
        format_sql: true
        use_sql_comments: true
=====================================================================